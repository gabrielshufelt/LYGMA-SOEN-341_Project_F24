<div class="container hall-building">
	<div class="gradient-filter">
		<header class="title teams">
			<h1><%= team.name %></h1>
		</header>
		<section class="instructor-dashboard-teams">
      <table class="table">
        <thead>
          <tr>
            <th>Course</th>
            <th>Members</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td><%= team.course_name %></td>
            <td>
              <% if team.students.count > 0 %>
                <% team.students.each do |s| %>
                  <%= s.first_name + ' ' + s.last_name %><br/>
                <% end %>
              <% else %>
                No members.
              <% end %>
            </td>
          </tr>
        </tbody>
      </table>
      <%= link_to "Back to teams", teams_instructor_dashboard_index_path %>
		</section>
	</div>
</div>